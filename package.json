{
  "name": "css-bouncer",
  "version": "0.0.0",
  "description": "Bouncer is a CSS Linter using CSS AST and a focus on pluggable rules. Bouncer keeps delinquents out of you CSS.",
  "license": "MIT",
  "author": {
    "name": "Danny Fritz",
    "email": "dannyfritz@gmail.com",
    "url": "http://dannyfritz.com"
  },
  "main": "index.js",
  "bin": {
    "cssb": "bin/cmd.js",
    "css-bouncer": "bin/cmd.js"
  },
  "files": [
    "index.js",
    "bin",
    "lib"
  ],
  "scripts": {
    "stare": "npm-stare",
    "check": "npm run lint && npm run test",
    "lint": "eslint lib test",
    "test": "tape test/*.js | tap-dot"
  },
  "stare": {
    "check": {
      "path": [
        "lib",
        "test",
        "bin",
        "index.js"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dannyfritz/css-bouncer"
  },
  "homepage": "https://github.com/dannyfritz/css-bouncer",
  "bugs": {
    "url": "https://github.com/dannyfritz/css-bouncer/issues"
  },
  "keywords": [
    "css",
    "lint",
    "linter",
    "hint"
  ],
  "dependencies": {
    "backbone-events-standalone": "^0.2.4",
    "lodash": "^2.4.1",
    "postcss": "^2.2.5"
  },
  "devDependencies": {
    "npm-stare": "^1.0.2",
    "tap-dot": "^0.2.3",
    "tape": "^3.0.0"
  }
}
